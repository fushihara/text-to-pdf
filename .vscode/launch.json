{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "サーバーサイドを起動",
      "runtimeArgs": [
        "--loader",
        "ts-node/esm",
        "--no-warnings=ExperimentalWarning"
      ],
      "args": [
        "${workspaceFolder}/src/server-side/server.ts"
      ],
      "console": "internalConsole",
      "outputCapture": "std",
      "env": {
        "BASE_DIR": "/test/"
      }
    }
  ]
}